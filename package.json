{
    "name": "vscode-ocamlformat",
    "displayName": "ocamlformat",
    "description": "`ocamlformat` binding on vscode",
    "publisher": "Shuumatsu",
    "repository": "https://github.com/Shuumatsu/vscode-ocamlformat",
    "version": "0.1.3",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.ocamlformat",
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.ocamlformat",
                "title": "ocamlformat"
            }
        ]
    },
    "configuration": {
        "type": "object",
        "title": "OCaml formatter configuration",
        "properties": {
            "ocamlformat.path": {
                "type": "string",
                "default": "ocamlformat",
                "description": "The path to the `ocamlformat` binary."
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/command-exists": "^1.2.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.1.8",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^7.0.0",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vsce": "^1.71.0",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "command-exists": "^1.2.8"
    }
}
